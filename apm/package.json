{
  "name": "apm-agent",
  "version": "latest",
  "description": "APM Agent for blockchain clients",
  "main": "build/app.js",
  "scripts": {
    "test": "nyc mocha --reporter=list --exit  build/monit/index.spec.js",
    "nomad-dev": "nomad agent -dev --config scripts/nomad.hcl",
    "lint": "eslint 'src/**/*.js' -c .eslintrc.js --ignore-pattern node_modules/ --fix",
    "docker:prebuild": "grunt pre-test && pkg --targets=node10-alpine-x64 package.json --options trace-warnings",
    "docker:prebuild-macos": "grunt pre-test && pkg --targets=node10-macos-x64 package.json --options trace-warnings",
    "build": "npm run docker:prebuild && docker build -t app-apm-agent:$npm_package_version . && rm apm-agent",
    "deploy-mac": "PASSWORD=$(aws ecr get-login-password 2>&1) && docker login -u AWS -p $PASSWORD https://218989195752.dkr.ecr.ap-southeast-1.amazonaws.com && docker tag app-apm-agent:$npm_package_version 218989195752.dkr.ecr.ap-southeast-1.amazonaws.com/app-apm-agent:$npm_package_version && docker push 218989195752.dkr.ecr.ap-southeast-1.amazonaws.com/app-apm-agent:$npm_package_version",
    "deploy-linux": ". ~/genv/bin/activate && sudo $(aws ecr get-login --no-include-email) && sudo docker tag app-apm-agent:$npm_package_version 218989195752.dkr.ecr.ap-southeast-1.amazonaws.com/app-apm-agent:$npm_package_version && sudo docker push 218989195752.dkr.ecr.ap-southeast-1.amazonaws.com/app-apm-agent:$npm_package_version"
  },
  "bin": "build/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stakewithus/apm-agent.git"
  },
  "keywords": [
    "apm",
    "blockchain"
  ],
  "author": "cryptobabybear",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/stakewithus/apm-agent/issues"
  },
  "homepage": "https://github.com/stakewithus/apm-agent#readme",
  "dependencies": {
    "bignumber.js": "^8.1.1",
    "bluebird": "^3.5.5",
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "cron": "^1.7.2",
    "dotenv": "^6.2.0",
    "ioredis": "^4.9.5",
    "js-sha256": "^0.9.0",
    "json-stable-stringify": "^1.0.1",
    "longjohn": "^0.2.12",
    "markdown-table": "^1.1.3",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "nock": "^10.0.6",
    "pino": "^5.12.3",
    "pkg": "^5.2.1",
    "rate-limiter-flexible": "^1.0.2",
    "request": "^2.88.0",
    "slonik": "^16.19.0",
    "twilio": "^3.34.0",
    "uuid": "^3.3.2",
    "ws": "^7.0.0",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "grunt": "^1.0.3",
    "grunt-babel": "^7.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "mocha": "^6.1.4",
    "mocha-steps": "^1.3.0",
    "nyc": "^14.1.0",
    "redis-mock": "^0.43.0",
    "sinon": "7.2.2",
    "supertest": "^4.0.2",
    "tap": "^13.1.2"
  }
}
