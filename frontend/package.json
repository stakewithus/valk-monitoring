    "apexcharts": "3.10.0",
    "axios": "0.19.0",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.4",
    "classnames": "2.2.6",
    "core-js": "2.6.5",
    "js-cookie": "2.2.1",
    "jwt-decode": "2.2.0",
    "lodash.get": "4.4.2",
    "moment": "2.24.0",
    "randomcolor": "0.5.4",
